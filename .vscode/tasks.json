{
  "tasks": [
  {
    "type": "cppbuild",
    "label": "C/C++: g++.exe build active file",
    "command": "C:\\msys64\\ucrt64\\bin\\g++.exe",
    "args": [
      "-fdiagnostics-color=always",
      "-g",
      "${file}",
      "-I",
      "${fileWorkspaceFolder}\\Inc\\",
      "-Wall",
      "-o",
      "${fileWorkspaceFolder}\\build\\${fileBasenameNoExtension}.exe"
    ],
    "options": {
      "cwd": "${fileDirname}"
    },
    "problemMatcher": [
      "$gcc"
    ],
    "group": "build",
    "detail": "Task generated by Debugger."
  },
  {
    "type": "cmake",
    "label": "CMake: Build Project",
    "command": "build",
    "targets": [
      "all"
    ],
    "group": {
      "kind": "build",
      "isDefault": true
    },
    "problemMatcher": [],
    "detail": "CMake template build task"
  },
  {
    "type": "shell",
    "label": "CMake: Re-build project",
    "command": "cmake",
    "args": [
      "--build",
      "${command:cmake.buildDirectory}",
      "--clean-first",
      "-v"
    ],
    "options": {
      "cwd": "${workspaceFolder}"
    },
    "problemMatcher": [
      "$gcc"
    ]
  },
  {
    "type": "shell",
    "label": "CMake: Clean project",
    "command": "cmake",
    "args": [
      "--build",
      "${command:cmake.buildDirectory}",
      "--target",
      "clean"
    ],
    "options": {
      "cwd": "${workspaceFolder}"
    },
    "problemMatcher": []
  },
],
  "version": "2.0.0"
}